{
    "env": {
      "browser": true,
      "es6": true,
      "commonjs": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 9,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "react/prop-types": 0,
      "react/jsx-uses-vars": [2],
      "no-console": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "no-param-reassign": 0,
      "react/forbid-prop-types": 0,
      "arrow-parens": 0,
      "lines-between-class-members": 0,
      "react/destructuring-assignment": 0
    }
  }